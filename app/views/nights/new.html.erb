<h1>New Night Out</h1>

<%= form_for @night do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :date %>
  <%= f.date_field :date %>

  <%= f.label :time %>
  <%= f.time_field :time %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit "Time to Head Out!" %>
<% end %>
